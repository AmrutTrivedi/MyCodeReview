
public interface Printer {
int a=12;
void print();
}
interface Shower{
	void show();
}
class Applet implements Printer,Shower{
	public void print() {
		System.out.println("Hello Printer");
	}
	public void show(){
	System.out.println("Hello Shower");
	}
public static void main(String args[]) {
		Applet A1 = new Applet();
		A1.print();
		A1.show();
}
}