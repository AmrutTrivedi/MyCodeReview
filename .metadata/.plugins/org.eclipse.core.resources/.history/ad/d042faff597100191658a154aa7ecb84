import java.util.ArrayList;

public class StudentDataBase {
public static void main(String args[]) {
	ArrayList<String> al = new ArrayList<String>();
	al.add(0,"Amit");
	al.add(1,"Amrut");
	al.add(2,"Ram");
	ArrayList<String> al2 = new ArrayList<String>();
	al2.add(0,"Amit12@gmail.com" );
	al2.add(1,"amrut31@gmail.com");
	al2.add(2,"Ram");

	for(String s:al) {
		System.out.println("The List of Students are: "+s);
	}
	for(String s:al2) {
		System.out.println("The List of Students EmailId: "+s);
	}
}
}
