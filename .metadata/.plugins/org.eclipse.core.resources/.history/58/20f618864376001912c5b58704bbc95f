package MyPackage;

import java.util.Arrays;
import java.util.Scanner;

public class MyClass1 {
	
private static int[] id;
String name;
String email;
int percentage;

public MyClass1(int[] id, String name,String email,int percentage)
{
	this.id=Arrays.copyOf(id, id.length);
	this.name=name;
	this.email=email;
	this.percentage=percentage;
	
}

public static int[] getId()
{
	return id;
		
}

void setId()
{
	int[] id = {1,2,3,4,5};
}

}