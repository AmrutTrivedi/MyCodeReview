import java.util.Scanner;
public class Encap {
private String name;
String getName()
{
	return name;	
}
void setName()
{
	name = "John";
	
}
public static void main(String args[]) {
	Encap c1 = new Encap();
	Scanner Sc = new Scanner(System.in);
	String name = Sc.nextLine();
	System.out.println(name);
}}
